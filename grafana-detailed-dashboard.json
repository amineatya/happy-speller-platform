{
  "dashboard": {
    "id": null,
    "title": "Happy Speller Platform - Comprehensive Metrics",
    "tags": ["happy-speller", "platform", "detailed-monitoring", "infrastructure", "application"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "15s",
    "time": {
      "from": "now-3h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "üèóÔ∏è Infrastructure Status Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"node-exporter\"}",
            "legendFormat": "{{instance}} Status"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "üöÄ Jenkins Pipeline Health",
        "type": "stat",
        "targets": [
          {
            "expr": "jenkins_builds_last_build_result_ordinal{job_name=~\".*happy-speller.*\"}",
            "legendFormat": "{{job_name}}"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "SUCCESS",
                    "color": "green"
                  },
                  "1": {
                    "text": "UNSTABLE",
                    "color": "yellow"
                  },
                  "2": {
                    "text": "FAILURE",
                    "color": "red"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "‚ò∏Ô∏è Kubernetes Pods Health",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(kube_pod_status_phase{namespace=\"demo\", phase=\"Running\"})",
            "legendFormat": "Running Pods"
          },
          {
            "expr": "sum(kube_pod_status_phase{namespace=\"demo\", phase=\"Failed\"})",
            "legendFormat": "Failed Pods"
          },
          {
            "expr": "sum(kube_pod_status_phase{namespace=\"demo\", phase=\"Pending\"})",
            "legendFormat": "Pending Pods"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "üåê Application Response Time",
        "type": "gauge",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"happy-speller\"}[5m])) * 1000",
            "legendFormat": "95th Percentile"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "unit": "ms",
            "min": 0,
            "max": 1000,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 200
                },
                {
                  "color": "red",
                  "value": 500
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "üíª CPU Usage by Node",
        "type": "timeseries",
        "targets": [
          {
            "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "{{instance}} CPU %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "none",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 4,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              }
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "üß† Memory Usage by Node",
        "type": "timeseries",
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "{{instance}} Memory %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "none",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 4,
              "showPoints": "never",
              "spanNulls": false
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "red",
                  "value": 95
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "üóÑÔ∏è Disk Usage",
        "type": "bargauge",
        "targets": [
          {
            "expr": "100 - ((node_filesystem_avail_bytes{mountpoint=\"/\",fstype!=\"rootfs\"} * 100) / node_filesystem_size_bytes{mountpoint=\"/\",fstype!=\"rootfs\"})",
            "legendFormat": "{{instance}} Root FS"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 12
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "üåê Network I/O",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(node_network_receive_bytes_total{device!~\"lo|veth.*|docker.*|flannel.*|cali.*|cbr.*\"}[5m]) * 8",
            "legendFormat": "{{instance}} - {{device}} - RX"
          },
          {
            "expr": "rate(node_network_transmit_bytes_total{device!~\"lo|veth.*|docker.*|flannel.*|cali.*|cbr.*\"}[5m]) * 8",
            "legendFormat": "{{instance}} - {{device}} - TX"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 12
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "never",
              "spanNulls": false
            },
            "unit": "bps"
          }
        }
      },
      {
        "id": 9,
        "title": "üî• Load Average",
        "type": "stat",
        "targets": [
          {
            "expr": "node_load1",
            "legendFormat": "{{instance}} - 1m"
          },
          {
            "expr": "node_load5",
            "legendFormat": "{{instance}} - 5m"
          },
          {
            "expr": "node_load15",
            "legendFormat": "{{instance}} - 15m"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 12
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "decimals": 2,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "‚ò∏Ô∏è Kubernetes Cluster Detailed Overview",
        "type": "table",
        "targets": [
          {
            "expr": "kube_node_info",
            "legendFormat": "{{node}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 18
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": false,
              "displayName": "Node"
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "node": "Node Name",
                "kernel_version": "Kernel Version",
                "kubelet_version": "Kubelet Version",
                "kubeproxy_version": "Kube-Proxy Version",
                "container_runtime_version": "Runtime Version"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        }
      },
      {
        "id": 11,
        "title": "üöÄ Application HTTP Requests",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"happy-speller\"}[5m])) by (method, status_code)",
            "legendFormat": "{{method}} - {{status_code}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 26
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "showPoints": "never",
              "spanNulls": false
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 12,
        "title": "‚è±Ô∏è Application Response Time Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(http_request_duration_seconds_bucket{service=\"happy-speller\"}[5m])",
            "legendFormat": "{{le}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 26
        },
        "options": {
          "calculate": false,
          "yBucketBound": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        }
      },
      {
        "id": 13,
        "title": "üîç Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"happy-speller\", status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total{service=\"happy-speller\"}[5m])) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 34
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        }
      },
      {
        "id": 14,
        "title": "üìä Request Volume",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"happy-speller\"}[5m]))",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 34
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps",
            "decimals": 1
          }
        }
      },
      {
        "id": 15,
        "title": "‚ö° Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "(time() - process_start_time_seconds{service=\"happy-speller\"}) / 86400",
            "legendFormat": "Uptime Days"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 34
        },
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "unit": "d",
            "decimals": 1,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "green",
                  "value": 7
                }
              ]
            }
          }
        }
      },
      {
        "id": 16,
        "title": "üîó Active Connections",
        "type": "gauge",
        "targets": [
          {
            "expr": "sum(http_connections_active{service=\"happy-speller\"})",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 34
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "unit": "short",
            "min": 0,
            "max": 1000,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 500
                },
                {
                  "color": "red",
                  "value": 800
                }
              ]
            }
          }
        }
      },
      {
        "id": 17,
        "title": "üèóÔ∏è Jenkins Build History",
        "type": "timeseries",
        "targets": [
          {
            "expr": "jenkins_builds_duration_milliseconds_summary{job_name=~\".*happy-speller.*\"}",
            "legendFormat": "{{job_name}} - Build Duration"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 38
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "bars",
              "fillOpacity": 80,
              "gradientMode": "none",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "never",
              "spanNulls": false
            },
            "unit": "ms"
          }
        }
      },
      {
        "id": 18,
        "title": "üéØ Jenkins Success Rate",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(jenkins_builds_last_build_result_ordinal{job_name=~\".*happy-speller.*\"} == 0)",
            "legendFormat": "Success"
          },
          {
            "expr": "sum(jenkins_builds_last_build_result_ordinal{job_name=~\".*happy-speller.*\"} == 1)",
            "legendFormat": "Unstable"
          },
          {
            "expr": "sum(jenkins_builds_last_build_result_ordinal{job_name=~\".*happy-speller.*\"} == 2)",
            "legendFormat": "Failure"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 38
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "displayLabels": ["name", "value"]
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        }
      },
      {
        "id": 19,
        "title": "üì± Service Health Endpoints",
        "type": "table",
        "targets": [
          {
            "expr": "probe_success",
            "legendFormat": "{{instance}}"
          },
          {
            "expr": "probe_duration_seconds",
            "legendFormat": "{{instance}}_duration"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 38
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "instance": "Service",
                "Value": "Status",
                "probe_duration_seconds": "Response Time (s)"
              }
            }
          }
        ],
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "color-background"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 20,
        "title": "üóÉÔ∏è Database Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "mysql_global_status_connections{service=\"happy-speller-db\"}",
            "legendFormat": "DB Connections"
          },
          {
            "expr": "mysql_global_status_queries{service=\"happy-speller-db\"}",
            "legendFormat": "DB Queries/sec"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 44
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "showPoints": "never",
              "spanNulls": false
            }
          }
        }
      },
      {
        "id": 21,
        "title": "üìä Container Resource Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(container_cpu_usage_seconds_total{namespace=\"demo\"}) by (pod)",
            "legendFormat": "{{pod}} CPU"
          },
          {
            "expr": "sum(container_memory_usage_bytes{namespace=\"demo\"}) by (pod) / 1024 / 1024",
            "legendFormat": "{{pod}} Memory (MB)"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 44
        },
        "fieldConfig": {
